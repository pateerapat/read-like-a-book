<script>
    var app = new Vue({
        el: '#app',
        data: {
            bookList: [],
        },
        async mounted() {
            const response = await axios({
                method: "POST",
                url: "https://read-like-a-book-api.herokuapp.com/owned-book/get/user/all",
                data: {
                    id: document.getElementById("id").value,
                },
            })
            .then(
                (response) => {
                    return response.data.payload.data;
                },
            );
            this.bookList = response;
        },
    });
</script>